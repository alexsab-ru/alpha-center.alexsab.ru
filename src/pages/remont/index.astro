---
import PageLayout from '@/layouts/PageLayout.astro';

const description = 'Мультибрендовый официальный сервис в Самаре и Поволжье по ремонту американских, европейских и корейских автомобилей концерна General Motors. Выгодные цены, оригинальные запчасти, диагностика и ТО.';
const title = 'Ремонт авто General Motors в Самаре - цены';
const h1 = 'Сервисный центр';
import RemontModels from '@/components/RemontModels/RemontModels.astro';

export const breadcrumb = { name: "Сервисный центр", href: "/remont/" };
---

<PageLayout
  h1={h1}
  title={title}
  description={description}
  breadcrumb={[breadcrumb]}
>

<RemontModels />

</PageLayout>